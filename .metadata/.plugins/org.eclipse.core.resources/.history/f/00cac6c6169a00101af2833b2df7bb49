#define NVIC_ISER0 0xE000E100
#define NVIC_ISPR0 0XE000E200

#define USART_IRQNo 39

void USART3_IRQHandler(void)
{

}

int main(void)
{
    /* Loop forever */

	uint32_t *pISPR1 = (uint32_t*) NVIC_ISPR0 + 4;

	*pISPR1 |= (1<< (USART_IRQNo % 32));

